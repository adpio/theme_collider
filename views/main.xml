<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>
		<!-- === Services Page === -->
		<template name="Main Page" id="website.main" page="True">
			<t t-call="website.layout">
				<div id="wrap">
					<div class="container">
						<!-- Begin Jumbotron -->
						<div class="jumbotron jumbotron-main" id="home">
							<div id="particles-js"></div><!-- /.particles div -->
							<div class="container center-vertically-holder">
								<div class="center-vertically">
									<div class="col-sm-8 col-sm-offset-2 col-lg-6 col-lg-offset-3 text-center">
										<img class="img-responsive scaleReveal" src="images/logo.png">
											<hr class="bottomReveal">
												<p class="bottomReveal">
													powered by innovation from all around the world<br>
													<small>presented by <b>Innovation Collider</b></small>
												</p>
												<a href="#ideology" data-id="ideology" class="scroll-link">
													<div class="scroll-indicator rotateBottomReveal">
														<span class="ion-mouse" style="font-size: 2em;"></span><br>
														<span class="ion-android-arrow-down arrow-scroll-indicator" style="font-size: 2em;"></span>
													</div>
												</a>
											</div><!-- /.column -->
										</div><!-- /.vertical center -->
									</div><!-- /.container -->
								</div>
								<!-- End Jumbotron -->
								<!-- Begin Intro -->
								<section id="ideology">

									<!-- Begin Hello Intro -->
									<div id="hello-intro" class="pt60 pb60">
										<div class="container">
											<div class="row">
												<div class="col-lg-2 col-sm-3">
													<h2 class="no-margin rotateLeftReveal">Hello.</h2>
												</div>
												<div class="col-lg-10 col-sm-9 mt30-xs">
													<h3 class="no-margin rightReveal">
														Your business constantly requires new technology to stay competitive.
														We source and deliver proven business software to help you create a better service for your customers, provide cost advantage and grow faster than ever. 
													</h3>
												</div>
											</div><!-- /.row -->
										</div><!-- /.container -->
									</div><!-- /.div -->
									<!-- End Hello Intro -->

									<!-- Begin Blog Posts Title -->
									<div id="our-process-title" class="pt30 pb30">
										<div class="container">
											<div class="row">
												<div class="col-lg-12 text-center">
													<h4 class="no-margin">Our Recent Articles</h4>
												</div>
											</div><!-- /.row -->
										</div><!-- /.container -->
									</div><!-- /.div -->
									<!-- End Blog Posts Title -->

									<!-- Begin Blog Posts -->
									<div id="our-process">
										<div class="container-fluid">
											<div class="row" id="blog-posts">

												<script type="text/x-handlebars-template" id="blog-post-template">
													{{#each blog-post}}

													<!-- Item: {{title}} -->
													<div class="col-sm-4 col-md-4 col-lg-4 process-box text-center pt50 pb50">
														<div class="leftReveal">
															<div class="process-intro">
																<h3 class="process-number" style="white-space: nowrap; overflow: hidden;">{{ title }}</h3>
																<div>
																	<img src="{{ img-link }}" style="width:100%; height: 300px;" />
																</div>
																<h2>{{ title }}</h2>
															</div>
															<div class="process-content">
																<h3>{{ title }} <small>( {{ date }} )</small></h3>
																<hr/>
																<div>
																	<a href="{{ link }}"><img src="{{ img-link }}" style="max-height: 100px;" /></a>
																</div>
																<br/>
																<p class="no-margin" style="text-align: justify">
																	{{ synopsis }}
																</p>
																<br/><br/>
																<a class="btn btn-success" href="{{ link }}">read more</a>
															</div>
														</div><!-- /.animation -->
													</div><!-- /.column -->
													{{/each}}
												</script>
												<script type="text/javascript">
													var templateScript = Handlebars.compile($('#blog-post-template').html());
													var html = templateScript(context)
													$('#blog-posts').append(html)
												</script>
											</div><!-- /.row -->
										</div><!-- /.container -->
									</div><!-- /.div -->
									<!-- End Blog Posts -->
								</section><!-- End Intro Section -->




								<!-- Begin Tech Partners -->
								<section id="work" class="background1 section-padding-top">
									<div class="container">
										<div class="row">
											<div class="col-xs-12 section-title text-center">
												<h2>Our Technology Partners</h2>
												<span class="section-divider mb15"></span>
												<p class="mb30 scaleReveal">Check out proven solutions that can deliver measurable results today.</p>
											</div><!-- /.column -->
										</div><!-- /.row -->
										<div class="row">
											<div class="col-xs-12 text-center mb50">
												<!-- Filter Buttons -->
												<button class="filter btn btn-default btn-category btn-lg" data-filter="all">All</button>
												<button class="filter btn btn-default btn-category btn-lg" data-filter=".awareness">Spark awareness</button>
												<button class="filter btn btn-default btn-category btn-lg" data-filter=".consideration">Trigger consideration</button>
												<button class="filter btn btn-default btn-category btn-lg" data-filter=".conversion">Secure conversion</button>
												<button class="filter btn btn-default btn-category btn-lg" data-filter=".loyalty">Create loyalty</button>

												<!-- Sort Buttons -->
		<!-- 					<button class="sort btn btn-default btn-sort btn-lg" data-sort="my-order:asc"><span class="ion-ios-plus-empty"></span></button>
		<button class="sort btn btn-default btn-sort btn-lg" data-sort="my-order:desc"><span class="ion-ios-minus-empty"></span></button> -->
	</div><!-- /.column -->
</div><!-- /.row -->
<div id="thework">




	<script type="text/x-handlebars-template" id="product-template">
		{{#each product}}

		<!-- Item: {{name}} -->
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 portfolio-box no-padding mix {{filters}}" data-my-order="1">
			<div class="portfolio-image-holder"><img src="{{img}}" alt="{{name}}-img" class="img-responsive portfolio-image"></div>
			<span class="portfolio-badge badge">{{name}}</span>
			<span class="portfolio-hover">
				<span>
					<span class="project-title no-margin-bottom mt10">{{tagline}}</span>
					<button type="button" class="btn btn-default mt10" data-toggle="modal" data-target="#{{id}}-services" >Case Studies & More</button>

				</span>
			</span>
			<!-- Item 1 Modal -->
			<div class="modal fade" id="{{id}}-services" tabindex="-1" role="dialog">
				<div class="modal-dialog">
					<div class="modal-content">
						<div class="modal-header">
							<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="ion-ios-close-empty"></span>
						</button>
						{{# if img}}	
						<img src="{{ img }}"/ style="max-height: 100px; max-width: 100px; float: right;">
							{{ else }}
							<span class="pe-7s-airplay services-icon-2"></span>
							{{/if}}
							<h4 class="service-title">{{h4}}</h4><small>with {{name}}</small>
						</div>
						<div class="modal-body">
							{{{description}}}
							<h5 class="heading-1 modal-heading mb20">More Information</h5>
							<p class="no-margin-bottom">
								<a class="btn btn-primary btn-lg" href="/page/case-studies">
									<span class="ion-easel portfolio-links-icons"></span> 
									Case Studies
								</a>
								{{#if infographic-link}}
								<a href="{{infographic-link}}" target="_blank" class="btn btn-primary btn-lg">
									<span class="ion-archive portfolio-links-icons"></span> 
									Download infographic
								</a>
								{{/if}}
								{{#if reveal-link}}
								<a href="{{reveal-link}}" target="_blank" class="btn btn-primary btn-lg">
									<span class="ion-map portfolio-links-icons"></span> Virtual Tour
								</a>
								{{/if}}
							</p>
							<hr>
								{{#if infographic-img}}
								<p>
									<img src="{{infographic-img}}" alt="{{name}} image alt" style="max-width:100%;">
									</p>
									{{/if}}
								</div>
							</div><!-- /.modal-content -->
						</div><!-- /.modal-dialog -->
					</div><!-- /.modal -->

				</div><!-- /.column -->

				{{/each}}
			</script>
			<script type="text/javascript">
				var templateScript = Handlebars.compile($('#product-template').html());
				var html = templateScript(context)
				$('#thework').append(html)
			</script>


		</div><!-- /.row -->
	</div><!-- /.container -->
</section><!-- /.section -->
<!-- End Tech Partners -->

</section><!-- /.section -->
<!-- End Intro -->


<!-- Begin Map Title -->
<div id="our-process-title" class="pt30 pb30">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h4 class="no-margin">We are GLOBAL</h4>
			</div>
		</div><!-- /.row -->
	</div><!-- /.container -->
</div><!-- /.div -->
<!-- End Map Title -->	

<!-- Begin Map -->
<div id="map"></div>
<!-- End Map -->

<!-- Begin Network -->
<section id="network" class="background2 section-padding">
	<div class="container">
		<div class="row">
			<div class="col-xs-12 section-title text-center">
				<h2>Innovation Network</h2>
				<span class="section-divider"></span>
			</div><!-- /.column -->
		</div><!-- /.row -->
		<div class="col-sm-12" style="text-align: right;">
			<!-- Buttons -->
			<h5 class="heading-1 mb10">Access to Innovation Network Services</h5>
			<p class="text-small mb15">Community of digital marketers</p>
			<p>

				<a href="/slides" class="btn btn-lg btn-success">Presentations</a>
				<a href="/blog" class="btn btn-lg btn-info">Blog</a>
				<a href="/event" class="btn btn-lg btn-warning">Events</a>
				<a href="/page/contactus" class="btn btn-lg btn-danger">Contact Us</a>

			</p>
			<br/><br/><br/>
		</div>
		<div id="thenetwork">

		</div>

		<script type="text/x-handlebars-template" id="network-template">
			<h3 class="heading-1 mb20">Innovation Directors</h3>
			<div class="row">	
				{{#each director}}


				<!-- Item {{id}} -->
				<div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 mt30">
					<div class="services-box leftReveal" id="{{id}}">
						<span class="services-badge badge">{{location}}</span>
						<img src="{{img}}" class="collider-profile" alt="{{id}}">
							<h3 class="service-name">{{name}}</h3>
							<p class="text-small"></p>
						</div>
					</div>
					<!-- /.column -->	


					{{/each}}
				</div>
				<br><br>
					<h3 class="heading-1 mb20">Innovation Advisors</h3>
					<div class="row">
						{{#each advisor}}


						<!-- Item {{id}} -->
						<div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 mt30">
							<div class="services-box leftReveal" id="{{id}}">
								<span class="services-badge badge">{{location}}</span>
								<img src="{{img}}" class="collider-profile" alt="{{id}}">
									<h3 class="service-name">{{name}}</h3>
									<p class="text-small"></p>
								</div>
							</div>
							<!-- /.column -->


							{{/each}}
						</div>

					</script>
					<script type="text/javascript">
						var templateScript = Handlebars.compile($('#network-template').html());
						var html = templateScript(context)
						$('#thenetwork').append(html)
					</script>


				</div><!-- /.container -->
			</section><!-- /.section -->
			<!-- End Network -->
			<!-- Begin Blog Posts Title -->
			<div id="our-process-title" class="pt30 pb30">
				<div class="container">
					<div class="row">
						<div class="col-lg-12 text-center">
							<h4 class="no-margin">Our Approach</h4>
						</div>
					</div><!-- /.row -->
				</div><!-- /.container -->
			</div><!-- /.div -->
			<!-- End Blog Posts Title -->
			<!-- Begin Our Process -->
			<div id="our-process">
				<div class="container-fluid">
					<div class="row">
						<div class="col-sm-4 col-md-4 col-lg-4 process-box text-center pt50 pb50">
							<div class="leftReveal">
								<div class="process-intro">
									<h3 class="process-number">01</h3>
									<div><span class="process-icons pe-7s-rocket rotateBottomReveal"></span></div>
									<h2>Built from the Best</h2>
								</div>
								<div class="process-content">
									<div><span class="process-icons pe-7s-rocket"></span></div>
									<h2 class="mt15 mb20">Built from the Best</h2>
									<p class="no-margin">Our portfolio focuses on marketing technology and team organization solutions with proven track records and clear advantages over trending methodology. </p>
								</div>
							</div><!-- /.animation -->
						</div><!-- /.column -->
						<div class="col-sm-4 col-md-4 col-lg-4 process-box text-center pt50 pb50">
							<div class="bottomReveal">
								<div class="process-intro">
									<h3 class="process-number">02</h3>
									<div><span class="process-icons pe-7s-diamond rotateBottomReveal"></span></div>
									<h2>Hand Selected</h2>
								</div>
								<div class="process-content">
									<div><span class="process-icons pe-7s-diamond"></span></div>
									<h2 class="mt15 mb20">Hand Selected</h2>
									<p class="no-margin">All brands in our portfolio pass our rigorous inclusion criteria. We fill our portfolio with business tech solutions that deliver on their promises with repeatable results. </p>
								</div>
							</div><!-- /.animation -->
						</div><!-- /.column -->
						<div class="col-sm-4 col-md-4 col-lg-4 process-box text-center pt50 pb50 no-border-right">
							<div class="rightReveal">
								<div class="process-intro">
									<h3 class="process-number">03</h3>
									<div><span class="process-icons pe-7s-like2 rotateBottomReveal"></span></div>
									<h2>Future Friendly</h2>
								</div>
								<div class="process-content">
									<div><span class="process-icons pe-7s-like2"></span></div>
									<h2 class="mt15 mb20">Future Friendly</h2>
									<p class="no-margin">We care for our clients, and continue to monitor critical KPIs and maximize their impact. We continue to provide effective business technology as we discover further innovation being created by young companies.</p>
								</div>
							</div><!-- /.animation -->
						</div><!-- /.column -->
					</div><!-- /.row -->
				</div><!-- /.container -->
			</div><!-- /.div -->
			<!-- End Our Process -->


			<!-- Begin About -->
			<section id="about" class="background1 section-padding">
				<div class="container">
					<div class="row">
						<div class="col-xs-12 section-title text-center">
							<h2>About</h2>
							<span class="section-divider"></span>
						</div><!-- /.column -->
					</div><!-- /.row -->
					<div class="row mb15">
						<div class="col-sm-5 leftReveal">
							<h5 class="heading-1 mb20">Business Solutions That Work</h5>
							<h3 class="mb15 no-margin-top">Powered by innovation from around the world</h3>
							<p class="no-margin text-small">We put innovation on the market. We source the best business technology for our portfolio, and inform our clients how to benefit from it. We use social selling to get directly in touch with key decision makers to bring change faster.</p>
						</div><!-- /.column -->
						<div class="col-sm-7 mt30-xs rightReveal">
							<h5 class="heading-1 mb20">How we started it</h5>
							<span class="icon-handle-streamline-vector logo-about"></span>
							<p>The rate of adoption of new technology has to be increased if we are to benefit from the new discoveries of our technological pioneers. In a cafe in Barcelona, 3 entrepreneurs of the IESE Business School, joined minds to tackle this head-on, and from it, Innovation Collider was formed.</p>
							<p>We created Innovation Collider, a business software marketplace powered by social selling</p>
						</div><!-- /.column -->
					</div><!-- /.row -->
					<div class="row mb30">
						<div class="col-sm-9 leftReveal">
							<h5 class="heading-1 mb20">Why?</h5>
							<p class="lead no-margin">Fascinating technology is being discovered and created all around us, and we look to secure those innovations that best serve our clients needs. Our current portfolio focuses on marketing technology that: 
								<ul class="list-arrow list-arrow-color no-margin">
									<li>Creates awareness</li>
									<li>Sparks consideration</li>
									<li>Secures conversations</li>
									<li>Creates loyal customers</li>
								</ul>
								To help our clients understand that business drivers and potential benefits of new technology, we have created a socially powered marketplace to deliver change to those in our network.<br /><br />
								<h5>Read more on our <a href="/blog" class="btn btn-success">Innovation Blog</a></h5>
							</p>
						</div><!-- /.column -->
						<div class="col-sm-3 mt30-xs rightReveal">
							<h5 class="heading-1 mb20">How can we help you?</h5>
							<p class="no-margin text-small"><i>“Tell us your story and we will determine if there is any new technology for you to benefit from. We help improve marketing and acquisition funnels with the implementation of new technology.”</i></p>
						</div><!-- /.column -->
					</div><!-- /.row -->

				</div><!-- /.container -->
			</section><!-- /.section -->
			<!-- End About -->

			<!-- Begin Team -->
			<section id="team" class="background2 section-padding">
				<div class="container">
					<div class="row">
						<div class="col-xs-12 section-title text-center">
							<h2>Team</h2>
							<span class="section-divider"></span>
						</div><!-- /.column -->
					</div><!-- /.row -->
					<div class="row">
						<div class="col-sm-4 leftReveal">
							<h5 class="heading-1 mb20">Innovation Collider</h5>
							<h3 class="no-margin-top mb15">Business software that works</h3>
							<p class="no-margin text-small">powered by innovation from all around the world</p>
						</div><!-- /.column -->
						<div class="col-sm-8 mt20-xs rightReveal">
							<h5 class="heading-1 mb20">Who are we?</h5>
							<p>
								Our team consists of 3 entrepreneurs from the IESE Business School in Barcelona. We share a love for technology, and the opportunities it creates for the future. Innovation Collider was created to spread these opportunities.
							</p>
						</div><!-- /.column -->
					</div><!-- /.row -->
					<div class="row mb40">

						<!-- Item 1 -->
						<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 topReveal">
							<div class="team-member-box" id="AdamPiotrowski">
								<h3>Adam Piotrowski</h3>
								<hr>
									<div class="team-image-wrapper">
										<span class="team-image-hover">
											<span class="team-hover-content">
												<!-- Social -->
												<span><span class="ion-android-share-alt social-icon-title"></span></span>
												<a href="https://twitter.com/iCollider"><span class="ion-social-twitter social-icons-light-hover mr15"></span></a>
												<a href="https://www.facebook.com/InnovationCollider"><span class="ion-social-facebook social-icons-light-hover mr15"></span></a>

											</span>
										</span>
										<img src="images/Adam.png" alt="1 Member" class="img-responsive img-circle team-member-img">
										</div>
										<hr>
											<p class="text-xs-small">
												Adam is a serial entrepreneur, having started 2
												previous companies, 1 successfully. His innate
												passion for math, technology, and business has
												lead him to an intersection: the delivery and use
												of technology for complex problems. Adam also
												has a background in consulting with PwC and
												KPMG, and private equity in the telecom
												industry.
												<br><br>
													During the IESE MBA program Adam helped
													many start-ups at Startupbootcamp in Barcelona
													on financials and product strategy.
												</p>

											</div>
										</div><!-- /.column -->

										<!-- Item 2 -->
										<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 mt40-xs bottomReveal">
											<div class="team-member-box" id="JerzyCiecierski">
												<h3>Jurek Ciecierski</h3>
												<hr>
													<div class="team-image-wrapper">
														<span class="team-image-hover">
															<span class="team-hover-content">
																<!-- Social -->
																<span class="ion-android-share-alt social-icon-title"></span>
																<a href="#"><span class="ion-social-twitter social-icons-light-hover mr15"></span></a>
																<a href="#"><span class="ion-social-facebook social-icons-light-hover mr15"></span></a>
																<a href="#"><span class="ion-social-googleplus-outline social-icons-light-hover"></span></a>
															</span>
														</span>
														<img src="images/Jurek.jpg" alt="2 Member" class="img-responsive img-circle team-member-img">
														</div>
														<hr>
															<p class="text-xs-small">
																Jerzy has a passion for bringing development to
																growing markets, having previously worked in the
																Polish, English, and Emirati banking sectors with HSBC.
																His current priority is to continue to developing
																economies by creating the right local opportunities.
																<br><br><br><br><br>
																	Having graduated for IESE, Jerzy looks to use the
																	inspiration he gained at IESE’s MBA program to
																	develop local communities.
																</p>

															</div>
														</div><!-- /.column -->

														<!-- Item 3 -->
														<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 mt40-sm mt40-xs topReveal">
															<div class="team-member-box" id="ErikLiepmann">
																<h3>Erik Liepmann</h3>
																<hr>
																	<div class="team-image-wrapper">
																		<span class="team-image-hover">
																			<span class="team-hover-content">
																				<!-- Social -->
																				<span><span class="ion-android-share-alt social-icon-title"></span></span>
																				<a href="#"><span class="ion-social-twitter social-icons-light-hover mr15"></span></a>
																				<a href="#"><span class="ion-social-facebook social-icons-light-hover mr15"></span></a>
																				<a href="#"><span class="ion-social-googleplus-outline social-icons-light-hover"></span></a>
																			</span>
																		</span>
																		<img src="images/Erik.jpg" alt="3 Member" class="img-responsive img-circle team-member-img">
																		</div>
																		<hr>
																			<p class="text-xs-small">
																				Having moved internationally his entire life, Erik
																				is addicted to new opportunities, and has a
																				passion for implementing sharing discoveries.
																				Reflecting this passion he has accumulated
																				experience from the hospitality, recording arts,
																				and pharmaceutical industries across a variety of
																				functions.
																				<br><br>
																					His most recent adventure has brought him to Barcelona and Warsaw, where the team works tirelessly to serve our clients and bring technology to the forefront of business.
																				</p>

																			</div>
																		</div><!-- /.column -->

																		<!-- Item 4 -->
																		<div class="col-xs-12 col-sm-6 col-md-3 col-lg-3 mt40-sm mt40-xs topReveal">
